<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TableRow
        android:layout_weight="0"
        android:gravity="center">

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit1"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="1"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_1" />

        </FrameLayout>

        <FrameLayout
            android:layout_gravity="center"
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit2"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="2"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_2" />
        </FrameLayout>

        <FrameLayout
            android:layout_gravity="center"
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit3"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="3"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_3" />
        </FrameLayout>
    </TableRow>

    <TableRow
        android:layout_weight="0"
        android:gravity="center">

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit4"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="4"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_4" />
        </FrameLayout>

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit5"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="5"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_5" />
        </FrameLayout>

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit6"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="6"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_6" />
        </FrameLayout>
    </TableRow>

    <TableRow
        android:layout_weight="0"
        android:gravity="center">

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit7"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="7"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_7" />
        </FrameLayout>

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit8"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="8"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_8" />
        </FrameLayout>

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit9"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="9"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_9" />
        </FrameLayout>
    </TableRow>

    <TableRow
        android:layout_weight="0"
        android:gravity="center">

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/DigitStar"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="*"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_asterisk" />
        </FrameLayout>

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/Digit00"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="0+"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_0" />
        </FrameLayout>

        <FrameLayout
            android:layout_weight="0"
            android:padding="20dp">

            <org.linphone.views.Digit
                android:id="@+id/DigitHash"
                style="@style/NethCTIDialpadDigit"
                android:layout_gravity="center"
                android:soundEffectsEnabled="true"
                android:text="#"
                android:textSize="0sp"
                app:icon="@drawable/ic_numpad_hashtag" />
        </FrameLayout>
    </TableRow>

</TableLayout>
